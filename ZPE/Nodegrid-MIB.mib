-- Information about configured devices

ngInfoDevices    OBJECT IDENTIFIER
-- STATUS current
-- DESCRIPTION This is the root for information about configured devices
::= { nodegridSystem 2 }

ngInfoDevicesNumOfDevices  OBJECT-TYPE
    SYNTAX     Integer32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
       "This object contains number of configured devices.
        This identifies the number of entries in ngInfoDevicestable."
::= { ngInfoDevices 1 }

ngInfoDevicesTable   OBJECT-TYPE
    SYNTAX  SEQUENCE OF NgInfoDevicesEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION        
       "This table represents configured devices in the unit."
::= { ngInfoDevices 2 }

ngInfoDevicesEntry   OBJECT-TYPE
    SYNTAX  NgInfoDevicesEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION        
        "An entry for each configured device. 
        Each entry contains information on device name, status, type, 
        mode and license status."
    INDEX   { ngDeviceType, ngDeviceUID }
::= { ngInfoDevicesTable 1}
 
NgInfoDevicesEntry ::=
    SEQUENCE {
        ngDeviceUID       Unsigned32,
        ngDeviceName      OCTET STRING,
        ngDeviceStatus    INTEGER,
        ngDeviceType      INTEGER,
        ngDeviceConnectThrough OCTET STRING,
        ngDeviceMode      INTEGER,
        ngDeviceLicensed  INTEGER
    }

ngDeviceUID     OBJECT-TYPE
    SYNTAX     Unsigned32(1..65000)
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION         
        "This object unique identifies the device."
::= { ngInfoDevicesEntry 1 }

ngDeviceName    OBJECT-TYPE
    SYNTAX     OCTET STRING
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Device name."
::= { ngInfoDevicesEntry 2 }

ngDeviceStatus    OBJECT-TYPE
    SYNTAX     INTEGER{
                unknown(0),
                inUse(1),
                connected(2),
                disconnected(3)
    }
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Device status."
::= { ngInfoDevicesEntry 3 }

ngDeviceType    OBJECT-TYPE
    SYNTAX     INTEGER{
                ilo(1),
                imm(2),
                drac(3),
                idrac6(4),
                ipmi15(5),
                ipmi20(6),
                ilom(7),
                cimcucs(8),
                netapp(9),
                deviceconsole(10),
                virtualconsolevmware(11),
                virtualconsolekvm(12),
                consoleserveracs(13),
                consoleserveropengear(14),
                consoleserveracs6000(15),
                consoleserverlantronix(16),
                kvmdsr(17),
                localserial(18),
                pdumph2(19),
                pdupm3000(20),
                pduraritan(21),
                pduservertech(22),
                pduapc(23),
                pdubaytech(24),
                pdueconnex(25),
                kvmmpu(26),
                usbserial(27),
                unknown(99)
    }
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION         
        "Device Type."
::= { ngInfoDevicesEntry 4 }

ngDeviceConnectThrough    OBJECT-TYPE
    SYNTAX     OCTET STRING
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "How unit is connected to device.
         Physical device name (ttySXX) for serial ports.
         IP address of the device for others types."
::= { ngInfoDevicesEntry 5 }

ngDeviceMode    OBJECT-TYPE
    SYNTAX     INTEGER{
                disabled(0),
                enabled(1),
                discovered(2),
                ondemand(3),
                licensed(4),
                unlicensed(5)
    }
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Device mode."
::= { ngInfoDevicesEntry 6 }

ngDeviceLicensed    OBJECT-TYPE
    SYNTAX     INTEGER{
                 unlicensed(0),
                 licensed(1)
    }
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Device license status."
::= { ngInfoDevicesEntry 7 }

-- Information about Open Sessions  
ngOpenSessions    OBJECT IDENTIFIER
-- STATUS current
-- DESCRIPTION This is the root for open sessions information
::= { nodegridSystem 3 }

ngOpenSessionsNumOfSessions  OBJECT-TYPE
    SYNTAX     Integer32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION        
       "This object contains number of open sessions.
        This identifies the number of entries in ngOpenSessionstable."
::= { ngOpenSessions 1 }

ngOpenSessionsTable   OBJECT-TYPE
    SYNTAX  SEQUENCE OF NgOpenSessionsEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION        
       "This table has information about open sessions in the unit."
::= { ngOpenSessions 2 }

ngOpenSessionsEntry   OBJECT-TYPE
    SYNTAX  NgOpenSessionsEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION        
        "An entry for each open session. 
        Each entry contains information on username used to logged in the session,
        connection mode, session type, device name when direct connect to device, 
        session start time."
    INDEX   { ngOpenSessionRef }
::= { ngOpenSessionsTable 1}
 
NgOpenSessionsEntry ::=
    SEQUENCE {
        ngOpenSessionRef      Unsigned32,
        ngOpenSessionUser     OCTET STRING,
        ngOpenSessionMode     INTEGER,
        ngOpenSessionType     INTEGER,
        ngOpenSessionSourceIP IpAddress,
        ngOpenSessionDevice   OCTET STRING,
        ngOpenSessionStartTime OCTET STRING
    }

ngOpenSessionRef     OBJECT-TYPE
    SYNTAX     Unsigned32(1..65000)
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION         
        "This object unique identifies the openned session."
::= { ngOpenSessionsEntry 1 }

ngOpenSessionUser     OBJECT-TYPE
    SYNTAX     OCTET STRING
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "User logged in the session."
::= { ngOpenSessionsEntry 2 }

ngOpenSessionMode     OBJECT-TYPE
    SYNTAX     INTEGER{
                ssh(0),
                telnet(1),
                console(2),
                http(3),
                https(4),
                raw(5),
                mks(6),
                cloud(7),
                dialin(8)
    }
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "how session is stablished."
::= { ngOpenSessionsEntry 3 }

ngOpenSessionType     OBJECT-TYPE
    SYNTAX     INTEGER{
                kvm(1),
                console(2),
                power(3),
                web(4),
                ipmi(5),
                cli(6),
                api(7),
                mks(8),
                tunnel(9)
    }
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Session type."
::= { ngOpenSessionsEntry 4 }

ngOpenSessionSourceIP     OBJECT-TYPE
    SYNTAX     IpAddress
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "When remote access, it has the IP address of the user machine."
::= { ngOpenSessionsEntry 5 }

ngOpenSessionDevice   OBJECT-TYPE
    SYNTAX     OCTET STRING
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Device Name of the accessed device within this session."
::= { ngOpenSessionsEntry 6 }

ngOpenSessionStartTime   OBJECT-TYPE
    SYNTAX     OCTET STRING
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Time when session started."
::= { ngOpenSessionsEntry 7 }

-- Information about events ocurrencies  
ngEventsList    OBJECT IDENTIFIER
-- STATUS current
-- DESCRIPTION This is the root for event counters
::= { nodegridSystem 4 }

ngEventsListNumOfEvents  OBJECT-TYPE
    SYNTAX     Integer32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION        
       "This object contains number of events supported by unit.
        This identifies the number of entries in ngEventsListtable."
::= { ngEventsList 1 }

ngEventsListTable   OBJECT-TYPE
    SYNTAX  SEQUENCE OF NgEventsListEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION        
       "This table has information about events ocurrencies in this unit."
::= { ngEventsList 2 }

ngEventsListEntry   OBJECT-TYPE
    SYNTAX  NgEventsListEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION        
        "An entry for each event generated by this unit. 
        Each entry contains information on event number, event description,
        ocurrencies, event category."
    INDEX   { ngEventNumber }
::= { ngEventsListTable 1}
 
NgEventsListEntry ::=
    SEQUENCE {
        ngEventNumber      Integer32,
        ngEventDescription OCTET STRING,
        ngEventOcurrencies Counter32,
        ngEventCategory    INTEGER
    }

ngEventNumber    OBJECT-TYPE
    SYNTAX     Integer32(1..65000)
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION         
        "This object unique identifies the event."
::= { ngEventsListEntry 1 }

ngEventDescription   OBJECT-TYPE
    SYNTAX     OCTET STRING
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Event Description."
::= { ngEventsListEntry 2 }

ngEventOcurrencies    OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Number of times this event was generated."
::= { ngEventsListEntry 3 }

ngEventCategory    OBJECT-TYPE
    SYNTAX     INTEGER{
                system(1),
                aaa(2),
                device(3),
                logging(4)
    }
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Event Category."
::= { ngEventsListEntry 4 }

-- Serial Ports Statistics  
ngNSCStats    OBJECT IDENTIFIER
-- STATUS current
-- DESCRIPTION This is the root for local serial ports statistics
::= { ngNSCSystem 1 }

ngNSCStatsNumOfPorts  OBJECT-TYPE
    SYNTAX     Integer32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION        
       "This object contains number of local serial ports available in this unit.
        This identifies the number of entries in ngNSCStatstable."
::= { ngNSCStats 1 }

ngNSCStatsTable   OBJECT-TYPE
    SYNTAX  SEQUENCE OF NgNSCStatsEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION        
       "This table has information about each local serial port in this unit."
::= { ngNSCStats 2 }

ngNSCStatsEntry   OBJECT-TYPE
    SYNTAX  NgNSCStatsEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION        
        "An entry for each local serial port in this unit. 
        Each entry contains information on device name, speed, rs232 signals status,
        serial statistcs (Rx/Tx bytes, CTS/DCD variation, ...)."
    INDEX   { ngNSCStatsPortNumber }
::= { ngNSCStatsTable 1}
 
NgNSCStatsEntry ::=
    SEQUENCE {
        ngNSCStatsPortNumber      Integer32,
        ngNSCStatsDeviceName      OCTET STRING,
        ngNSCStatsPortSpeed       Integer32,
        ngNSCStatsRxBytes         Counter32,
        ngNSCStatsTxBytes         Counter32,
        ngNSCStatsCTSVar          Counter32,
        ngNSCStatsDCDVar          Counter32,
        ngNSCStatsFrameE          Counter32,
        ngNSCStatsOverrun         Counter32,
        ngNSCStatsParityE         Counter32,
        ngNSCStatsBreakE          Counter32,
        ngNSCStatsBufferO         Counter32,
        ngNSCStatsDTR             INTEGER,
        ngNSCStatsRTS             INTEGER,
        ngNSCStatsDCD             INTEGER,
        ngNSCStatsCTS             INTEGER
    }

ngNSCStatsPortNumber    OBJECT-TYPE
    SYNTAX     Integer32(1..96)
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION         
        "This object unique identifies the local serial port."
::= { ngNSCStatsEntry 1 }

ngNSCStatsDeviceName    OBJECT-TYPE
    SYNTAX     OCTET STRING
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Device Name associated with this local serial port."
::= { ngNSCStatsEntry 2 }

ngNSCStatsPortSpeed    OBJECT-TYPE
    SYNTAX     Integer32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Actual speed of this local serial port."
::= { ngNSCStatsEntry 3 }

ngNSCStatsRxBytes    OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Number of bytes received from this local serial port since counter reset operation."
::= { ngNSCStatsEntry 4 }

ngNSCStatsTxBytes    OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Number of bytes transmitted to this local serial port since counter reset operation."
::= { ngNSCStatsEntry 5 }

ngNSCStatsCTSVar    OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Number of times the RS232 CTS signal goes from Down to UP in this local serial port 
         since counter reset operation."
::= { ngNSCStatsEntry 6 }

ngNSCStatsDCDVar    OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Number of times the RS232 DCD signal goes from Down to UP in this local serial port 
         since counter reset operation."
::= { ngNSCStatsEntry 7 }

ngNSCStatsFrameE    OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Number of framed errors in this local serial port since counter reset operation."
::= { ngNSCStatsEntry 8 }

ngNSCStatsOverrun    OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Number of detected overrun errors in this local serial port since counter reset operation."
::= { ngNSCStatsEntry 9 }

ngNSCStatsParityE    OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Number of parity errors in this local serial port since counter reset operation."
::= { ngNSCStatsEntry 10 }

ngNSCStatsBreakE    OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Number of break received in this local serial port since counter reset operation."
::= { ngNSCStatsEntry 11 }

ngNSCStatsBufferO    OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "Number of buffer overrun errors this local serial port since counter reset operation."
::= { ngNSCStatsEntry 12 }

ngNSCStatsDTR        OBJECT-TYPE
    SYNTAX     INTEGER{
                down(0),
                up(1)
    }
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "State of RS232 DTR signal in this local serial port."
::= { ngNSCStatsEntry 13 }

ngNSCStatsRTS        OBJECT-TYPE
    SYNTAX     INTEGER{
                down(0),
                up(1)
    }
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "State of RS232 RTS signal in this local serial port."
::= { ngNSCStatsEntry 14 }

ngNSCStatsDCD        OBJECT-TYPE
    SYNTAX     INTEGER{
                down(0),
                up(1)
    }
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "State of RS232 DCD signal in this local serial port."
::= { ngNSCStatsEntry 15 }

ngNSCStatsCTS        OBJECT-TYPE
    SYNTAX     INTEGER{
                down(0),
                up(1)
    }
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION         
        "State of RS232 CTS signal in this local serial port."
::= { ngNSCStatsEntry 16 }

-- SFP transceiver info  
ngNSCSfp    OBJECT IDENTIFIER
-- STATUS current
-- DESCRIPTION This is the root for SFP info
::= { ngNSCSystem 2 }

ngNSCSfpInfoTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF NgNSCSfpInfoEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "table with SFP info"
    ::= { ngNSCSfp 1 }

ngNSCSfpInfoEntry OBJECT-TYPE
    SYNTAX      NgNSCSfpInfoEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "SFP info - part of the content of the SFP's 0xA0 EEPROM"
    INDEX   { ifIndex }
    ::=  { ngNSCSfpInfoTable 1 }

NgNSCSfpInfoEntry ::=
    SEQUENCE {
        ngNSCSfpInfoIfName      OCTET STRING,
        ngNSCSfpInfoId		OCTET STRING,
	ngNSCSfpInfoIdExt	OCTET STRING,
	ngNSCSfpInfoConnector	OCTET STRING,
	ngNSCSfpInfoTransCode	OCTET STRING,
	ngNSCSfpInfoTransType	OCTET STRING,
	ngNSCSfpInfoEnconding	OCTET STRING,
	ngNSCSfpInfoRateId	OCTET STRING,
	ngNSCSfpInfoLaserWave	Integer32,
	ngNSCSfpInfoVendorName	OCTET STRING,
	ngNSCSfpInfoVendorOUI	OCTET STRING,
	ngNSCSfpInfoVendorPn	OCTET STRING,
	ngNSCSfpInfoDOM		TruthValue,
        ngNSCSfpInfoAlarm	TruthValue
    }

ngNSCSfpInfoIfName OBJECT-TYPE
	SYNTAX OCTET STRING 
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	"system interface name"
::= { ngNSCSfpInfoEntry 1 }

ngNSCSfpInfoId OBJECT-TYPE
	SYNTAX OCTET STRING 
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
