Nodegrid-MIB DEFINITIONS ::= BEGIN

IMPORTS
  MODULE-IDENTITY, OBJECT-TYPE FROM SNMPv2-SMI
  Integer32, Counter32, Counter64, IpAddress, Unsigned32 FROM SNMPv2-SMI
  MODULE-COMPLIANCE, OBJECT-GROUP FROM SNMPv2-CONF
  ifIndex FROM IF-MIB
  DisplayString, TruthValue FROM SNMPv2-TC
  zpeSystemsMibs FROM ZPESystems-MIB
      ;

nodegridMib MODULE-IDENTITY
        LAST-UPDATED    "202002240000Z"
        ORGANIZATION    "ZPE Systems Inc."
        CONTACT-INFO
            "ZPE Systems Support
             E-mail: support@zpesystems.com"
        DESCRIPTION
            "Nodegrid Family Vendor MIB definition.
             Adopted by Nodegrid release 4.1.0."
        REVISION        "202002240000Z"
        DESCRIPTION
            "Added new branch for ngSRSystem for Nodegrid SR products."
        REVISION        "201908280000Z"
        DESCRIPTION
            "First revision."
::= { zpeSystemsMibs 1 }

nodegridSystem  OBJECT IDENTIFIER ::= { nodegridMib 1 }

ngNSCSystem	OBJECT IDENTIFIER ::= { nodegridMib 2 }

