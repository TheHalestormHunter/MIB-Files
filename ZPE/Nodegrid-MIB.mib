-- Switch Interfaces Statistics  
ngSwitchInterfaceStats    OBJECT IDENTIFIER
-- STATUS current
-- DESCRIPTION This is the root for switch interface statistics
::= { ngSwitchSystem 1 }

ngSwitchInterfaceStatsNumOfInterfaces  OBJECT-TYPE
    SYNTAX     Integer32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
       "This object contains number of switch interfaces available in this unit.
        This identifies the number of entries in ngSwitchInterfaceStatstable."
::= { ngSwitchInterfaceStats 1 }

ngSwitchInterfaceStatsTable   OBJECT-TYPE
    SYNTAX  SEQUENCE OF NgSwitchInterfaceStatsEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
       "This table has statistics plus information about each switch interfaces in this unit."
::= { ngSwitchInterfaceStats 2 }

ngSwitchInterfaceStatsEntry   OBJECT-TYPE
    SYNTAX  NgSwitchInterfaceStatsEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "An entry for each switch interface in this unit. 
        Each entry contains information on interface name, speed, status,
        statistcs (In/Out octets, In/Out unicast pkts, ...)."
    INDEX   { ngSwitchInterfaceStatsSlotNumber, ngSwitchInterfaceStatsIfNumber }
::= { ngSwitchInterfaceStatsTable 1}

NgSwitchInterfaceStatsEntry ::=
    SEQUENCE {
        ngSwitchInterfaceStatsSlotNumber       Integer32,
        ngSwitchInterfaceStatsIfNumber         Integer32,
        ngSwitchInterfaceStatsIfName           OCTET STRING,
        ngSwitchInterfaceStatsIfDescr          OCTET STRING,
        ngSwitchInterfaceStatsIfStatus         INTEGER,
        ngSwitchInterfaceStatsLinkState        INTEGER,
        ngSwitchInterfaceStatsIfSpeed          Integer32,
        ngSwitchInterfaceStatsInUnicastPkts    Counter64,
        ngSwitchInterfaceStatsInMulticastPkts  Counter64,
        ngSwitchInterfaceStatsInBroadcastPkts  Counter64,
        ngSwitchInterfaceStatsInOctets         Counter64,
        ngSwitchInterfaceStatsOutUnicastPkts   Counter64,
        ngSwitchInterfaceStatsOutMulticastPkts Counter64,
        ngSwitchInterfaceStatsOutBroadcastPkts Counter64,
        ngSwitchInterfaceStatsOutOctets        Counter64
	}

ngSwitchInterfaceStatsSlotNumber    OBJECT-TYPE
    SYNTAX     Integer32(0..96)
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
        "This object identifies the slot where the switch card is."
::= { ngSwitchInterfaceStatsEntry 1 }

ngSwitchInterfaceStatsIfNumber    OBJECT-TYPE
    SYNTAX     Integer32(1..16)
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
        "This object with ngSwitchInterfaceStatSlotNumber unique identifies the switch interface."
::= { ngSwitchInterfaceStatsEntry 2 }

ngSwitchInterfaceStatsIfName    OBJECT-TYPE
    SYNTAX     OCTET STRING
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "Interface Name associated with this switch interface."
::= { ngSwitchInterfaceStatsEntry 3 }

ngSwitchInterfaceStatsIfDescr    OBJECT-TYPE
    SYNTAX     OCTET STRING
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "Configured Switch Interface Description."
::= { ngSwitchInterfaceStatsEntry 4 }

ngSwitchInterfaceStatsIfStatus    OBJECT-TYPE
    SYNTAX     INTEGER {
                disabled(0),
                enabled(3)
    }
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "Admin status of this switch  interface."
::= { ngSwitchInterfaceStatsEntry 5 }

ngSwitchInterfaceStatsLinkState    OBJECT-TYPE
    SYNTAX     INTEGER {
                unknow(0),
                down(1),
		up(2)
    }
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "Actual link state of this switch  interface."
::= { ngSwitchInterfaceStatsEntry 6 }

ngSwitchInterfaceStatsIfSpeed    OBJECT-TYPE
    SYNTAX     Integer32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "Actual speed of this switch  interface."
::= { ngSwitchInterfaceStatsEntry 7 }

