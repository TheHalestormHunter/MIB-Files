ngNSCSfpInfoTransCode	OBJECT-TYPE
	SYNTAX OCTET STRING 
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	"transceiver code"
::= { ngNSCSfpInfoEntry 5 }

ngNSCSfpInfoTransType	OBJECT-TYPE
	SYNTAX OCTET STRING
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	"transceiver type"
::= { ngNSCSfpInfoEntry 6 }

ngNSCSfpInfoEnconding	OBJECT-TYPE
	SYNTAX OCTET STRING 
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	"transceiver enconding"
::= { ngNSCSfpInfoEntry 7 }

ngNSCSfpInfoRateId OBJECT-TYPE
	SYNTAX OCTET STRING 
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	"transceiver rate id"
